<%= form_for @url,url: urls_path,html: {method: 'post'}  do |f| %>
	<div class="form_format">

		<div class="form-group">
			<%= f.label :original_url %>
			<%= f.text_field :original_url, class: "form-control"%>
		</div>
		
		<div class="form-group">			
			<%= f.submit 'Submit', class: "btn btn-primary" %>
		</div>	
	</div>	
<% end %>